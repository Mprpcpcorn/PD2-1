def get_srv_dict(node_list):
    rezultats = {}
    for node in node_list:
        dati = node.split(";")
        hostname = dati[0]
        ip = dati[1]
        status = dati[3].strip().upper()
        
       
        if hostname.startswith("Srv"):
           
            rezultats[ip] = status
            
  return rezultats


